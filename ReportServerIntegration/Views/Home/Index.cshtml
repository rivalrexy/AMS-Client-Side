@using DevExpress.DashboardWeb
@model ReportServerIntegration.Models.DashboardViewerModel

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




    @Html.DevExpress().GetStyleSheets(
       new StyleSheet { ExtensionSuite = ExtensionSuite.Dashboard }
    )



    @Html.DevExpress().GetScripts(
        new Script { ExtensionSuite = ExtensionSuite.Dashboard }
    )


@Html.DevExpress().Dashboard(settings =>
{
    settings.Name = "Dashboard";
    settings.WorkingMode = WorkingMode.ViewerOnly;
    settings.InitialDashboardId = Model.DashboardId;
    settings.BackendOptions.Uri = Model.DesignerUri;
    settings.BackendOptions.RequestHttpHeaders.Add("Authorization", Model.AuthorizationHeader);
}).GetHtml()

